<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
    <!-- Site Properities-->
    <title>Manticore CMS</title>
    <script src="js/sortablejs/Sortable.min.js"></script>
    <link rel="stylesheet" type="text/css" href="semantic.min.css">
    <script src="https://code.jquery.com/jquery-2.1.4.min.js">   </script>
    <script src="semantic.min.js">  </script>
    <link rel="stylesheet" type="text/css" href="home/page.css">
    <script src="home/page.js"></script>
    <title>$data.title</title>
  </head>
  <body>
    <follow-menu ui="" menu="data.top.menu"></follow-menu>
    <!-- try: labeled icon-->
    <sidebar-menu ui="left inverted labeled" menus="data.sidebar.menus"></sidebar-menu>
    <div class="pusher">
      <menu-container ui="top-menu sticky center aligned">
        <top-menu menu="data.top.menu"></top-menu>
      </menu-container>
      <div class="ui vertical segment">
        <div class="ui middle aligned stackable grid container">
          <div class="row">
            <div class="eight wide column">           
              <ui-list list="data.lists">
                <div class="item">
                  <ui-header label="Top story"></ui-header>
                  <form class="ui form">
                    <div class="ui list">
                      <div class="item">
                        <div class="right floated content">
                          <div class="ui button"><i class="remove icon"></i>                        Remove</div>
                        </div><img src="http://lorempixel.com/200/200/sports/" class="ui avatar image">
                        <div class="content"><a class="header"> 
                            <div class="ui mini input">
                              <input value="Victory">
                            </div></a>
                          <div class="description">Dark horse wins the race                  </div>
                        </div>
                      </div>
                      <div class="item">
                        <div class="right floated content">
                          <div class="ui button"><i class="remove icon"></i>                        Remove</div>
                        </div><img src="http://lorempixel.com/200/200/sports/" class="ui avatar image">
                        <div class="content"><a class="header"> 
                            <div class="ui mini input">
                              <input value="Victory">
                            </div></a>
                          <div class="description">Dark horse wins the race                  </div>
                        </div>
                      </div>
                    </div>
                  </form>
                </div>
              </ui-list>
            </div>
          </div>
        </div>
      </div>
    </div>
    <browser-refresh enabled="true"></browser-refresh>
  </body>
</html>